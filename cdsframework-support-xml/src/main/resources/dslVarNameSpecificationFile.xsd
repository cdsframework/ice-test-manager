<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:rl4="org.cdsframework.util.support.data.rule.dsl.var.name"
           xmlns:rl5="org.cdsframework.util.support.data.rule.dsl.var.group"
           targetNamespace="org.cdsframework.util.support.data.rule.dsl.var.name">
    <xs:import namespace="org.cdsframework.util.support.data.rule.dsl.var.group" schemaLocation="dslVarGroupSpecificationFile.xsd"/>
    <xs:complexType name="dslVarName">
        <xs:sequence>
            <xs:element name="cdsVersion" type="xs:string" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A cds version that this season is associated with.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="createDatetime" type="xs:dateTime"/>
            <xs:element minOccurs="0" name="createId" type="xs:string"/>
            <xs:element minOccurs="0" name="dslVarGroup" type="rl5:dslVarGroup"/>
            <xs:element minOccurs="0" name="dslResourceVarNameId" type="xs:int"/>
            <xs:element minOccurs="0" name="lastModDatetime" type="xs:dateTime"/>
            <xs:element minOccurs="0" name="lastModId" type="xs:string"/>
            <xs:element minOccurs="0" name="name" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="DslVarNameSpecificationFile" type="rl4:dslVarName"/>
</xs:schema>